page{
  background-color: rgb(61, 61, 61);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100%;
}
.Info{
  margin: 70rpx 50rpx 0 20rpx;
  color: white;
  font-size: xx-large;
  display: flex;
  flex-direction: column;
}
.Info view{
  font-size: large;
  margin-bottom: 10rpx;
}
.item{
  display: flex;
  flex-direction: row;
  color: white;
}
.SelectBite{
  margin: 100rpx 30rpx 0 30rpx;
  /* background: white; */
  border-radius: 50rpx;
}
.backgd{
  width: 100%;
  height: 100%;
  border-radius: 50rpx;
  position: fixed;
  z-index: -1;
}
.icon{
  flex: 1;
  width: 200rpx;
  height: 200rpx;
  margin:auto 10rpx;
}
.asset{
  text-align: center;
  color: rgb(172, 168, 168);
  margin-top: 30rpx;
}
.font{
  flex: 2;
  width: 50%;
}
.font view:first-child{
font-size: x-large;
margin: 80rpx 0 20rpx 50rpx;
font-weight: 700;
}
.font view:last-child{
  margin-left: 50rpx;
}